      <para id="id1169256632293">Koniese rat</para>
      <para id="id1169250902409">
        <figure id="id7976644">
          <media id="id7976644_media" alt="">
            <image mime-type="image/png" src="../../media/Picture 8-4deb.png" id="id7976644__onlineimage" height="167" width="152"/>
